unique_elements_string <- function(input_string) {
  unique_chars <- unique(strsplit(input_string, NULL)[[1]])
  return(unique_chars)
}
unique_numbers_vector <- function(input_vector) {
  unique_nums <- unique(input_vector)
  return(unique_nums)
}
example_string <- "hello world"
example_vector <- c(1, 2, 3, 2, 4, 1, 5, 6, 4)
unique_chars_result <- unique_elements_string(example_string)
unique_nums_result <- unique_numbers_vector(example_vector)
cat("Unique elements of the string:", unique_chars_result, "\n")
cat("Unique numbers of the vector:", unique_nums_result, "\n")
