english_letters <- letters
first_10_lower <- english_letters[1:10]
last_10_upper <- toupper(tail(english_letters, 10))
letters_between_22_24_upper <- toupper(english_letters[22:24])
cat("First 10 letters in lower case:", first_10_lower, "\n")
cat("Last 10 letters in upper case:", last_10_upper, "\n")
cat("Letters between 22nd to 24th positions in upper case:", letters_between_22_24_upper, "\n")
